<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CreateEventController">
   <children>
      <Label layoutX="145.0" layoutY="14.0" text="&quot;Create Event View&quot;">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="77.0" layoutY="109.0" text="Name:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="145.0" layoutY="109.0" promptText="Event Name" />
      <Button fx:id="submitEventBtn" layoutX="76.0" layoutY="331.0" mnemonicParsing="false" onAction="#submitEvent" prefHeight="100.0" prefWidth="100.0" text="Submit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="backBtn" layoutX="416.0" layoutY="456.0" mnemonicParsing="false" onAction="#backHome" prefHeight="30.0" prefWidth="70.0" text="Back">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="74.0" layoutY="153.0" text="Month:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="74.0" layoutY="198.0" text="Day:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="72.0" layoutY="237.0" text="Year:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="70.0" layoutY="279.0" text="Time:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="monthField" layoutX="145.0" layoutY="154.0" promptText="Enter Numerically" />
      <TextField fx:id="dayField" layoutX="145.0" layoutY="199.0" promptText="Enter Numerically" />
      <TextField fx:id="yearField" layoutX="145.0" layoutY="238.0" promptText="Enter Numerically" />
      <TextField fx:id="timeHRField" layoutX="145.0" layoutY="280.0" prefHeight="25.0" prefWidth="35.0" promptText="HR" />
      <TextField fx:id="timeMINField" layoutX="202.0" layoutY="280.0" prefHeight="25.0" prefWidth="39.0" promptText="MIN" />
      <Label layoutX="187.0" layoutY="279.0" text=":">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <RadioButton fx:id="amBtn" layoutX="251.0" layoutY="272.0" mnemonicParsing="false" selected="true" text="AM">
         <toggleGroup>
            <ToggleGroup fx:id="amPM" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="pmBtn" layoutX="251.0" layoutY="297.0" mnemonicParsing="false" text="PM" toggleGroup="$amPM" />
   </children>
</AnchorPane>
