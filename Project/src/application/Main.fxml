<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Label layoutX="93.0" layoutY="14.0" text="Event Scheduler">
         <font>
            <Font name="Arial Black" size="35.0" />
         </font></Label>
      <Button fx:id="createEventBtn" layoutX="72.0" layoutY="200.0" mnemonicParsing="false" onAction="#toCreateEvent" prefHeight="100.0" prefWidth="119.0" text="Create Event">
         <font>
            <Font name="Arial" size="13.0" />
         </font></Button>
      <Button fx:id="viewEventBtn" layoutX="310.0" layoutY="200.0" mnemonicParsing="false" onAction="#toViewEvent" prefHeight="100.0" prefWidth="119.0" text="View Events">
         <font>
            <Font name="Arial" size="13.0" />
         </font></Button>
   </children>
</AnchorPane>
